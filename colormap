function __cmap2()
{
echo -e "\n\n[01mÔº•ÔΩòÔΩîÔΩíÔΩÅ ÔΩÉÔΩèÔΩåÔΩèÔΩíÔΩì Ôºö[m\n"
xxx=16
while [ $xxx -lt 231 ]; do
	while [ $xxx -lt 100 ]; do
		echo -en "[01m "$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
		((xxx +=1))
		echo -en "[01m "$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
		((xxx +=1))
		echo -en "[01m "$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
		((xxx +=1))
		echo -en "[01m "$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
		((xxx +=1))
		echo -en "[01m "$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
		((xxx +=1))
		echo -e "[01m "$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
		((xxx +=1))
	done
	echo -en "[01m"$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
	((xxx +=1))
	echo -en "[01m"$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
	((xxx +=1))
	echo -en "[01m"$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
	((xxx +=1))
	echo -en "[01m"$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
	((xxx +=1))
	echo -en "[01m"$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
	((xxx +=1))
	echo -e "[01m"$xxx"[m:[00;38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m  "
	((xxx +=1))
done
}

function __cmap3v(){
xxx=232
echo -e "\n\nÔº≥ÔΩàÔΩÅÔΩÑÔΩÖÔΩì  ÔΩèÔΩÜ  ÔΩáÔΩíÔΩÖÔΩô (ÔΩñÔΩÖÔΩíÔΩîÔΩâÔΩÉÔΩÅÔΩå)Ôºö\n"

while [ $xxx -lt 256 ]; do
		echo -e "[38;5;15m "$xxx"   [48;5;"$xxx"m     [m  "
		((xxx +=1))
done
}

function __cmap3h(){
echo -e "\n\nÔº≥ÔΩàÔΩÅÔΩÑÔΩÖÔΩì  ÔΩèÔΩÜ  ÔΩáÔΩíÔΩÖÔΩô (ÔΩàÔΩèÔΩíÔΩâÔΩöÔΩèÔΩéÔΩîÔΩÅÔΩå)Ôºö\n"
if [ $COLUMNS -gt 95 ]; then
	h3count=232
	echo 2{32..55}
	while [ $h3count -lt 256 ]; do
		echo -en "\e[48;5;"$h3count"m   \e[m"
		echo -en " "
		h3count=$((h3count + 1))
	done
	echo -e "\n\n"
fi
}

function __cmapall(){
	__cmap1
	__cmap2
    if [ "$COLUMNS" -gt '97' ]; then
        __cmap3h
    else
        __cmap3v
    fi
    #echo -e ""
    figlet -f term -c -w 77 " ------ END OF FILE ------ "
    echo -e "\n\n"

}

function cmap(){
    __cmapall | less -r -P 'Space to advance a screen' -e
}

function __new_colors(){
	x=196
		echo -en "\033[38;5;0m\033[48;5;"$x"m "$x" \033[m\t"
