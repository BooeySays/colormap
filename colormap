			((xxx +=1))
			echo -en "[m "$xxx":[38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m "
			((xxx +=1))
			echo -en "[m "$xxx":[38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m "
			((xxx +=1))
			echo -en "[m "$xxx":[38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m "
			((xxx +=1))
			echo -en "[m "$xxx":[38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m "
			((xxx +=1))
			echo -e "BOLD / BRIGHT"
#		done
			echo -en "[m "$xxx":[38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m "
			((xxx +=1))
			echo -en "[m "$xxx":[38;5;"$xxx"m‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ‚ñÜ[m "
