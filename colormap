function __cmap1()
{
	xxx=0
	echo -e "\n\nÔº¢ÔΩÅÔΩìÔΩâÔΩÉ ÔΩÉÔΩèÔΩåÔΩèÔΩíÔΩìÔºö\n"
#	while [ "$xxx" -lt 17 ]; do
#		while [ "$xxx" -lt 8 ]; do
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -e "BOLD / BRIGHT"
#		done
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m"$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m"$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m"$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m"$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m"$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -en "[38;5;256m"$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
			echo -e "NORMAL"
#			echo -e """
#NOTE:
#0 - 7  are the same as 30 - 37 WITHOUT bold
#8 - 15 are the same colors are 30-37, when used with 01 (BOLD)

#"""
#	done
}

function __cmap1a()
{
	xxx=0
	echo -e "\n\nÔº¢ÔΩÅÔΩìÔΩâÔΩÉ ÔΩÉÔΩèÔΩåÔΩèÔΩíÔΩìÔºö\n"
	while [ "$xxx" -lt 17 ]; do
		while [ "$xxx" -lt 10 ]; do
			while [ "$xxx" -lt 8 ]; do
				echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
				((xxx +=1))
			done
			echo -e "BOLD / BRIGHT"
			echo -en "[38;5;256m "$xxx":[48;5;"$xxx"m       [m "
			((xxx +=1))
		done
		echo -en "[38;5;256m"$xxx":[48;5;"$xxx"m       [m "
		((xxx +=1))
	done
	echo -e "NORMAL"
}


#echo

function __cmap2()
{
echo -e "\n\nÔº•ÔΩòÔΩîÔΩíÔΩÅ ÔΩÉÔΩèÔΩåÔΩèÔΩíÔΩì Ôºö\n"
xxx=16
while [ $xxx -lt 231 ]; do
	while [ $xxx -lt 100 ]; do
		echo -en "[38;5;256m "$xxx": [48;5;"$xxx"m      [m  "
		((xxx +=1))
		echo -en "[38;5;256m "$xxx": [48;5;"$xxx"m      [m  "
		((xxx +=1))
		echo -en "[38;5;256m "$xxx": [48;5;"$xxx"m      [m  "
		((xxx +=1))
		echo -en "[38;5;256m "$xxx": [48;5;"$xxx"m      [m  "
		((xxx +=1))
		echo -en "[38;5;256m "$xxx": [48;5;"$xxx"m      [m  "
		((xxx +=1))
		echo -e "[38;5;256m "$xxx": [48;5;"$xxx"m      [m  "
		((xxx +=1))
	done
	echo -en "[38;5;256m"$xxx": [48;5;"$xxx"m      [m  "
	((xxx +=1))
	echo -en "[38;5;256m"$xxx": [48;5;"$xxx"m      [m  "
	((xxx +=1))
	echo -en "[38;5;256m"$xxx": [48;5;"$xxx"m      [m  "
	((xxx +=1))
	echo -en "[38;5;256m"$xxx": [48;5;"$xxx"m      [m  "
	((xxx +=1))
	echo -en "[38;5;256m"$xxx": [48;5;"$xxx"m      [m  "
	((xxx +=1))
	echo -e "[38;5;256m"$xxx": [48;5;"$xxx"m      [m  "
	((xxx +=1))
done
}

function __cmap3v(){
xxx=232
echo -e "\n\nÔº≥ÔΩàÔΩÅÔΩÑÔΩÖÔΩì  ÔΩèÔΩÜ  ÔΩáÔΩíÔΩÖÔΩô (ÔΩñÔΩÖÔΩíÔΩîÔΩâÔΩÉÔΩÅÔΩå)Ôºö\n"

while [ $xxx -lt 256 ]; do
		echo -e "[38;5;15m "$xxx"   [48;5;"$xxx"m     [m  "
		((xxx +=1))
done

}

function __cmap3h(){
echo -e "\n\nÔº≥ÔΩàÔΩÅÔΩÑÔΩÖÔΩì  ÔΩèÔΩÜ  ÔΩáÔΩíÔΩÖÔΩô (ÔΩàÔΩèÔΩíÔΩâÔΩöÔΩèÔΩéÔΩîÔΩÅÔΩå)Ôºö\n"
if [ $COLUMNS -gt 95 ]; then
	h3count=232
	echo 2{32..55}
	while [ $h3count -lt 256 ]; do
		echo -en "\e[48;5;"$h3count"m   \e[m"
		echo -en " "
		h3count=$((h3count + 1))
	done
	echo -e "\n\n"
fi
}
