function __cmap1()
{
	xxx=0
	echo -e "[01m\n\nÔº¢ÔΩÅÔΩìÔΩâÔΩÉ ÔΩÉÔΩèÔΩåÔΩèÔΩíÔΩìÔºö\n[m"
#	while [ "$xxx" -lt 17 ]; do
